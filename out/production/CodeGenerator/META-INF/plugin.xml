<idea-plugin version="2">
  <id>peak.plugin.android.code_generator</id>
  <name>Android code generator</name>
  <version>1.0</version>
  <vendor email="wenping0820@163.com" url="http://http://blog.csdn.net/wenping1980">Peak</vendor>

  <description><![CDATA[
      Enter short description for your plugin here.<br>
      <em>most HTML tags may be used</em>
    ]]></description>

  <change-notes><![CDATA[
      Add change notes here.<br>
      <em>most HTML tags may be used</em>
    ]]>
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="141.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <!-- uncomment to enable plugin in all products
  <depends>com.intellij.modules.lang</depends>
  -->

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action id="androidCodeGenerator" class="peak.plugin.android.codegenerator.AndroidCodeGenerator" text="Code generator"
            description="Android code generator">
      <add-to-group group-id="GenerateGroup" anchor="first"/>
      <keyboard-shortcut keymap="$default" first-keystroke="shift alt A"/>
    </action>
  </actions>

</idea-plugin>